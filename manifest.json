{
    "manifest_version": 2,
  
    "name": "Medgamers Channel Viewer",
    "description": "See who is online on our Mumble server",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "http://medgamers.duckdns.org:21803/1?callback=?"]
  }